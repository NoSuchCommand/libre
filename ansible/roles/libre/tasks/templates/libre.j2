application:
  type: "{{ config_libre.application.type }}"
  version: "{{ config_libre.application.version }}"
  verbose: "{{ config_libre.application.verbose }}"
  debug: "{{ config_libre.application.debug }}"
  ansible: "{{ r_libre_dest_ansible }}"
  source:
    repo: "{{ config_libre.application.source.repo }}"
    branch: "{{ config_libre.application.source.branch }}"
    base: "{{ config_libre.application.source.base }}"
    tmp: "{{ config_libre.application.source.tmp }}"
directory:
  base: "{{ config_libre.directory.base }}"
  course: "{{ config_libre.directory.course }}"
  collect: "{{ config_libre.directory.collect }}"
